    {
      "$type": "CLMS.AppDev.DomainModel.Models.FormControllerAction, CLMS.AppDev.DomainModel",
      "Code": "function void Save()\r\n{\r\n\tDomain.Intermediary newIntermediary = Domain.Intermediary.Create();\r\n\tnewIntermediary.Company = Model.Company;\r\n\tif( ! RegExpLib.VerbalExpressions.IsEmail(Model.EMail))\r\n\t{\r\n\t    ShowMessage(\"Please give a valid e-mail address.\", AppLib.MessageType.Error);\r\n\t    return;\r\n\t}\r\n\tnewIntermediary.EMail = Model.EMail;\r\n\tswitch Model.ChosenIntermediaryType \r\n        {\r\n        \tcase \"ThirdPartyLogisticsProvider\":\r\n        \t    newIntermediary.IntermediaryType = Domain.IntermediaryType.ThirdPartyLogisticsProviders;\r\n    \t    \tbreak;\r\n    \t   \tcase \"TransportationCompany\":\r\n    \t   \t    newIntermediary.IntermediaryType = Domain.IntermediaryType.TransportationCompanies;\r\n    \t    \tbreak;\r\n    \t    case \"CustomsAgent\":\r\n    \t        newIntermediary.IntermediaryType = Domain.IntermediaryType.CustomsAgents;\r\n    \t    \tbreak;\r\n    \t    case \"ShippingAgent\":\r\n    \t        newIntermediary.IntermediaryType = Domain.IntermediaryType.ShippingAgent;\r\n    \t    \tbreak;\r\n    \t    case \"Other\":\r\n    \t        newIntermediary.IntermediaryType = Domain.IntermediaryType.Other;\r\n    \t    \tbreak;\r\n//    \t    default:\r\n//    \t    \tnewIntermediary.IntermediaryType = Domain.IntermediaryType.Other;\r\n//    \t    \tbreak;\r\n    \t}\r\n    if(Model.ChosenIntermediaryType == \"Other\")\r\n    {\r\n        newIntermediary.OtherDescription = Model.Description;\r\n    }\r\n    try \r\n    {\r\n    \tnewIntermediary.Save();\t\r\n    }\r\n    catch Exception x {\r\n    \tDebugLib.Logger.WriteErrorLine(x);\r\n    \tShowMessage(\"Something went wrong.\", AppLib.MessageType.Error);\r\n    }\r\n    \r\n    Guid verificationGuid = Guid.CreateNew();\r\n    string code = verificationGuid.ToString();\r\n    \r\n    newIntermediary.GUID = code;\r\n    \r\n    string link = FormModels.SignUp.Controller.IntermediaryRender.GetLink(code);\r\n    CommonLib.EmailMessage emailMessage;\r\n\temailMessage.To = { Model.EMail };\r\n\temailMessage.Subject = \"TOME Finance Invitation\";\r\n\temailMessage.Body = \"You have been added to TOME Finance Platform as intermediary. Click <a href='\" + link + \"'>here</a> to sign up.\";\r\n\temailMessage.IsBodyHtml = true;\r\n\tCommonLib.Utilities.SendEmailAsync(emailMessage);\r\n    \r\n    CloseForm();\r\n}",
      "Name": "Save",
      "Permissions": [],
      "AllowAnonymous": false,
      "AllowAllAuthenticatedUsers": true,
      "AccessLog": false,
      "IsEntryPoint": false,
      "CausesValidation": true,
      "EventName": null
    }
  ],
